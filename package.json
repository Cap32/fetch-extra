{
  "name": "fetch-extra",
  "version": "0.0.0-2",
  "description": "extra methods and props for whatwg fetch and Request",
  "repository": "Cap32/fetch-extra",
  "scripts": {
    "prebuild": "rimraf ./dist && mkdirp ./dist && rimraf ./lib && mkdirp ./lib",
    "build:node": "babel src -o lib/index.js",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src -o dist/fetch-extra.cjs.js",
    "build:umd": "webpack src dist/fetch-extra.js",
    "build:min": "webpack -p src dist/fetch-extra.min.js",
    "build": "npm run build:cjs && npm run build:umd && npm run build:min && npm run build:node",
    "start": "npm run test:node -- -w",
    "test:browser": "babel-node test/utils/karma.js",
    "test:node": "mocha --require=babel-register --delay test/node.js",
    "test": "npm run test:node && npm run test:browser",
    "prepublish": "npm run test && npm run build"
  },
  "main": "lib/index.js",
  "browser": "dist/fetch-extra.cjs.js",
  "files": [
    "lib",
    "dist",
    "*.md"
  ],
  "keywords": [
    "fetch",
    "xhr",
    "http",
    "ajax",
    "promise",
    "request",
    "node"
  ],
  "author": "cap32",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^1.7.3",
    "tiny-querystring": "^0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-system-import-transformer": "^2.4.0",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "co-body": "^4.2.0",
    "cors": "^2.8.1",
    "cross-env": "^3.1.3",
    "delay": "^2.0.0",
    "eslint": "^4.12.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-webpack": "^2.0.6",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "output-host": "^1.1.0",
    "qs": "^6.3.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.9.1"
  }
}
