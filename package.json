{
  "name": "fetch-extra",
  "version": "0.0.0-3",
  "description": "extra methods and props for whatwg fetch and Request",
  "repository": "Cap32/fetch-extra",
  "scripts": {
    "prebuild": "rimraf lib umd min && mkdirp lib umd min",
    "build:node": "cross-env BABEL_ENV=node babel src -o lib/fetch-extra-node.js",
    "build:browser": "cross-env BABEL_ENV=browser babel src -o lib/fetch-extra-browser.js",
    "build:umd": "microbundle src --format umd -o umd --name fetchExtra --no-compress",
    "build:min": "microbundle src --format umd -o min --name fetchExtra",
    "build": "run-p build:*",
    "start": "npm run test:node -- -w",
    "test:browser": "cross-env BABEL_ENV=node babel-node test/utils/karma.js",
    "test:node": "cross-env BABEL_ENV=node mocha --require=babel-register --delay test/node.js",
    "test": "run-s test:*",
    "prepublish": "run-s test build"
  },
  "main": "lib/index.js",
  "browser": "lib/fetch-extra.cjs.js",
  "files": [
    "lib",
    "umd",
    "min",
    "*.md"
  ],
  "keywords": [
    "fetch",
    "xhr",
    "http",
    "ajax",
    "promise",
    "request",
    "node"
  ],
  "author": "cap32",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^1.7.3",
    "tiny-querystring": "^0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import-redirect": "^1.1.0",
    "babel-plugin-system-import-transformer": "^2.4.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "co-body": "^4.2.0",
    "cors": "^2.8.1",
    "cross-env": "^3.1.3",
    "delay": "^2.0.0",
    "eslint": "^4.12.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-webpack": "^2.0.6",
    "microbundle": "^0.2.4",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "output-host": "^1.1.0",
    "qs": "^6.3.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.9.1"
  }
}
