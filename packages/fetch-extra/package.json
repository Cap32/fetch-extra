{
	"name": "fetch-extra",
	"version": "0.1.1",
	"description": "extra methods and props for whatwg fetch and Request",
	"repository": "Cap32/fetch-extra",
	"scripts": {
		"prebuild": "rimraf lib && mkdirp lib",
		"build:node": "cross-env babel src -d lib --ignore whatwg-fetch",
		"build:umd": "cross-env BUILD_TARGET=umd rollup -c",
		"build:min": "cross-env BUILD_TARGET=min rollup -c",
		"build": "run-p build:*",
		"start": "npm run test:node -- -w",
		"test:browser": "cross-env BABEL_ENV=node babel-node test/utils/karma.js",
		"test:node": "cross-env BABEL_ENV=node nyc --reporter=lcov --reporter=text mocha --require=babel-register --delay test/node.js",
		"test": "run-s test:*",
		"coverage": "cat ./coverage/lcov.info | coveralls",
		"preversion": "run-s test build"
	},
	"main": "lib/fetch-extra-node.js",
	"browser": "lib/fetch-extra-browser.js",
	"files": [
		"lib",
		"*.md"
	],
	"engines": {
		"node": ">=4"
	},
	"keywords": [
		"fetch",
		"xhr",
		"http",
		"ajax",
		"promise",
		"request",
		"node"
	],
	"author": "cap32",
	"license": "MIT",
	"dependencies": {
		"node-fetch": "^2.1.2",
		"tiny-querystring": "^0.0.2"
	},
	"devDependencies": {
		"co-body": "^5.2.0",
		"cors": "^2.8.4",
		"delay": "^2.0.0",
		"karma": "1.x",
		"karma-chrome-launcher": "^2.2.0",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.5",
		"karma-rollup-preprocessor": "^6.0.0",
		"qs": "^6.5.2"
	}
}
