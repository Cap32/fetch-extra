{
	"name": "fetch-extra",
	"version": "0.2.0",
	"description": "extra methods and props for whatwg fetch and Request",
	"repository": "Cap32/fetch-extra",
	"scripts": {
    "prebuild": "rimraf lib es-browser browser dist && mkdirp lib es-browser browser dist",
    "postbuild": "rimraf es-browser",
    "browserbuild:es": "buble src/fetch-extra-browser -o es-browser --no modules",
    "browserbuild:cjs": "babel es-browser -d browser",
		"build:node": "cross-env BABEL_ENV=node babel src/fetch-extra-node -d lib",
		"build:browser": "run-s browserbuild:*",
		"build:umd": "cross-env BUILD_TARGET=umd rollup -c",
		"build:min": "cross-env BUILD_TARGET=min rollup -c",
		"build": "run-p build:*",
		"start": "jest --watchAll",
		"test:browser": "jest --testPathPattern=browser\\.test",
		"test:node": "jest --testPathPattern=node\\.test",
		"test": "jest",
		"preversion": "run-s test build"
	},
	"main": "lib/index.js",
	"browser": "browser/index.js",
	"files": [
		"lib",
		"dist",
		"browser",
		"*.md"
	],
	"engines": {
		"node": ">=4"
	},
	"keywords": [
		"fetch",
		"xhr",
		"http",
		"ajax",
		"promise",
		"request",
		"node"
	],
	"author": "cap32",
	"license": "MIT",
	"dependencies": {
		"fetch-extra-core": "^0.2.0",
		"node-fetch": "^2.1.2"
	},
	"devDependencies": {},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.js"
		],
		"modulePathIgnorePatterns": [
			"node_modules",
			"lib"
		]
	}
}
