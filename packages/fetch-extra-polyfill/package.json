{
	"name": "fetch-extra-polyfill",
	"version": "0.1.1",
	"description": "extra methods and props for whatwg fetch and Request with polyfill",
	"scripts": {
		"prebuild": "rimraf lib && mkdirp lib",
		"build:node": "cross-env babel src/node.js -o lib/fetch-extra-polyfill-node.js",
		"build:browser": "cross-env BUILD_TARGET=browser rollup -c",
		"build:umd": "cross-env BUILD_TARGET=umd rollup -c",
		"build:min": "cross-env BUILD_TARGET=min rollup -c",
		"build": "run-p build:*"
	},
	"main": "lib/fetch-extra-polyfill-node.js",
	"browser": "lib/fetch-extra-polyfill-browser.js",
	"files": [
		"lib",
		"*.md"
	],
	"keywords": [
		"fetch",
		"xhr",
		"http",
		"ajax",
		"promise",
		"request",
		"node"
	],
	"repository": "https://github.com/Cap32/fetch-extra",
	"author": "Cap32",
	"license": "MIT",
	"dependencies": {
		"fetch-extra": "^0.1.1",
		"whatwg-fetch": "^2.0.4"
	},
	"devDependencies": {
		"rollup": "^0.59.1",
		"rollup-plugin-alias": "^1.4.0",
		"rollup-plugin-buble": "^0.19.2",
		"rollup-plugin-commonjs": "^9.1.3",
		"rollup-plugin-es3": "^1.1.0",
		"rollup-plugin-filesize": "^1.5.0",
		"rollup-plugin-json": "^3.0.0",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-node-resolve": "^3.3.0",
		"rollup-plugin-uglify": "2.x"
	}
}
